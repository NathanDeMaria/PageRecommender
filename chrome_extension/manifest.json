{
  "name" : "Page Recommender",
  "version" : "0.1",
  "description" : "Mark pages as like/dislike to form recommendations.",
  "permissions": [
    "tabs",
    "storage",
    "http://*/",
    "https://*/"
  ],
  "options_page": "options.html",
  "browser_action": {
     "default_icon": "logo.png",
     "default_popup": "recommender.html"
  },
  "content_scripts": [
    {
      "matches": ["http://www.datatau.com/", "https://www.datatau.com/"], //"css": ["mystyles.css"],
      "js": ["jquery-2.1.4.min.js", "inline.js"]
    }
  ],
  "manifest_version": 2
}
